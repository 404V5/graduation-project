<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<welcome-file-list>
		<welcome-file>adminLogin.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>kehu</servlet-name>
		<servlet-class>com.bdglxt.action.kehu_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>kehu</servlet-name>
		<url-pattern>/kehu</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>bdlx</servlet-name>
		<servlet-class>com.bdglxt.action.bdlx_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bdlx</servlet-name>
		<url-pattern>/bdlx</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>jcxx</servlet-name>
		<servlet-class>com.bdglxt.action.jcxx_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>jcxx</servlet-name>
		<url-pattern>/jcxx</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>bdgl</servlet-name>
		<servlet-class>com.bdglxt.action.bdgl_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bdgl</servlet-name>
		<url-pattern>/bdgl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>jfxx</servlet-name>
		<servlet-class>com.bdglxt.action.jiaofei_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>jfxx</servlet-name>
		<url-pattern>/jfxx</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>bagl</servlet-name>
		<servlet-class>com.bdglxt.action.bagl_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bagl</servlet-name>
		<url-pattern>/bagl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>lpgl</servlet-name>
		<servlet-class>com.bdglxt.action.lipei_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>lpgl</servlet-name>
		<url-pattern>/lpgl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>yuangong</servlet-name>
		<servlet-class>com.bdglxt.action.yuangong_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>yuangong</servlet-name>
		<url-pattern>/yuangong</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.bdglxt.action.login_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
    <filter>
		<filter-name>SetChartEncoding</filter-name>
		<filter-class>com.bdglxt.util.EncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>SetChartEncoding</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
